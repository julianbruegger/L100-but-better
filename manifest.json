{
  "manifest_version": 2,
  "name": "L100 but Better",
  "version": "1.0",
  "description": "Customize CSS for example.com",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "browser_action": {
    "default_icon": "logo.jpg",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://activity.leuchterag.ch/*"],
      "js": ["contentScript.js"],
      "css": ["styles.css"]
    }
  ],  "web_accessible_resources": [
    "popup.js"
  ]
}
